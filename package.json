{
  "name": "cypress-e2e-tests",
  "version": "1.0.0",
  "description": "Cypress E2E Tests for edX applications",
  "repository": "https://github.com/edx/cypress-e2e-tests",
  "scripts": {
    "cy:open_admin_portal": "cypress open --env configFile=admin_portal_stage",
    "cy:run_admin_portal": "cypress run --env configFile=admin_portal_stage",
    "cy:open_journals": "cypress open --env configFile=journals_mit_stage",
    "cy:run_journals": "cypress run --env configFile=journals_mit_stage",
    "lint": "eslint --ext .js --ext .jsx .",
    "lint:fix": "eslint --ext .js --ext .jsx . --fix",
    "precommit": "npm run lint"
  },
  "dependencies": {
    "cypress": "^3.2.0",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "cypress-failed-log": "^2.4.0",
    "eslint": "^5.10.0",
    "eslint-config-edx": "^4.0.4",
    "eslint-plugin-cypress": "^2.1.2"
  }
}
