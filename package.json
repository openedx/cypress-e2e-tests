    "precommit": "npm run lint"
  },
  "dependencies": {
    "cypress": "^3.4.0",
    "cypress-failed-log": "^2.5.0",
    "eslint": "^6.0.1",
    "eslint-plugin-cypress": "^2.2.1",
    "uuid": "^3.3.2",
    "babel-eslint": "^10.0.2",
    "eslint-config-edx": "^4.0.4"
  }
}
