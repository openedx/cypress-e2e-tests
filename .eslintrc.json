{
  "plugins": [
    "cypress"
  ],
  "extends": [
    "plugin:cypress/recommended",
    "eslint-config"
  ],
  "parser": "babel-eslint",
  "env": {
    "cypress/globals": true
  },
  "rules": {
    "semi": ["error", "never"],
    "jsx-a11y/anchor-is-valid": [ "error", {
      "components": [ "Link" ],
      "specialLink": [ "to" ]
    }],
    "prefer-arrow-callback": 0,
    "func-names": ["error", "never"]
  }
}